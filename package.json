{
  "name": "reon-island-web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts -r @cypress/instrument-cra start",
    "build": "react-scripts build",
    "cy:open": "cypress open",
    "dev": "start-test 3000 cy:open",
    "server-build": "tsc --project ./server",
    "server-dev": "cd ./server && ts-node-dev src/main.ts",
    "server-start": "node ./server/dist/main.js",
    "storybook": "start-storybook -p 9009 -s public --ci",
    "storybook-build": "node ./node_modules/@storybook/react/bin/build.js -s public",
    "eject": "react-scripts eject",
    "test": "react-scripts test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "nyc": {
    "exclude": [
      "src/serviceWorker.ts",
      "src/setupTests.ts"
    ]
  },
  "dependencies": {
    "@cypress/code-coverage": "^1.12.2",
    "@cypress/instrument-cra": "^1.1.0",
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/addon-links": "^5.3.13",
    "@storybook/addons": "^5.3.13",
    "@storybook/preset-create-react-app": "^1.5.2",
    "@storybook/react": "^5.3.13",
    "@svgr/webpack": "^5.1.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/axios": "^0.14.0",
    "@types/bluebird": "^3.5.29",
    "@types/cypress": "^1.1.3",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest": "^25.1.2",
    "@types/koa": "^2.11.1",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-json": "^2.0.18",
    "@types/koa-router": "^7.4.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.1",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/soap": "^0.21.0",
    "apollo-boost": "^0.4.7",
    "apollo-server-koa": "^2.10.0",
    "axios": "^0.19.2",
    "bluebird": "^3.7.2",
    "cypress": "^4.0.2",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-json": "^2.0.2",
    "koa-router": "^8.0.7",
    "lodash": "^4.17.15",
    "node-sass": "^4.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.1",
    "soap": "^0.30.0",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^6.0.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.7.5"
  }
}
